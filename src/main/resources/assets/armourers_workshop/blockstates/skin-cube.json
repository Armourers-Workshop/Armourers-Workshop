{
  "multipart": [
    {
      "apply": {
        "model": "armourers_workshop:block/skin-cube"
      }
    },
    {
      "when": {
        "marker": "down",
        "has_marker": true
      },
      "apply": {
        "model": "armourers_workshop:block/skin-cube-marker",
        "x": 90
      }
    },
    {
      "when": {
        "marker": "up",
        "has_marker": true
      },
      "apply": {
        "model": "armourers_workshop:block/skin-cube-marker",
        "x": -90
      }
    },
    {
      "when": {
        "marker": "north",
        "has_marker": true
      },
      "apply": {
        "model": "armourers_workshop:block/skin-cube-marker"
      }
    },
    {
      "when": {
        "marker": "south",
        "has_marker": true
      },
      "apply": {
        "model": "armourers_workshop:block/skin-cube-marker",
        "y": 180
      }
    },
    {
      "when": {
        "marker": "west",
        "has_marker": true
      },
      "apply": {
        "model": "armourers_workshop:block/skin-cube-marker",
        "y": -90
      }
    },
    {
      "when": {
        "marker": "east",
        "has_marker": true
      },
      "apply": {
        "model": "armourers_workshop:block/skin-cube-marker",
        "y": 90
      }
    }
  ]
}
